@model IEnumerable<AquaTrack.Models.OldestFishResult>

<h2>Top 5 Oldest Fish</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Species</th>
            <th>Age</th>
            <th>Tank ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var fish in Model)
        {
            <tr>
                <td>@fish.Name</td>
                <td>@fish.Species</td>
                <td>@fish.Age</td>
                <td>@fish.TankId</td>
            </tr>
        }
    </tbody>
</table>

<a class="btn btn-secondary" asp-action="Index">Back</a>
